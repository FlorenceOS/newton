const print = @import("../lib/print.n");
const syscalls = @import("../lib/os/syscalls.n");

const PinnedVector = @import("../lib/containers/pinned_vector.n").PinnedVector;

fn main() void {
    print.str("Hello, world!\n".&);
    print.decimal(u64, 69);
    print.decimal(u32, 69);
    print.flush();
    var vec: PinnedVector(u32, 1024) = undefined;
    vec.init();
    vec.clear_and_uncommit();
    vec.append_assume_capacity(1);
    syscalls.exit(0);
}
