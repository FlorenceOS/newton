const other = @import("otherfile.tco");

const SYS = struct {
    const WRITE = 1;
    const EXIT = 60;
};

fn main() u64 {
    const print = fn(buf: *const u8) u64 {
        return @syscall(SYS.WRITE, 1, buf, other.strlen(buf));
    };

    print("Hello, world!\n".&);
    return @syscall(SYS.EXIT, 0);
}
